{
  "name": "corteza-server-corredor",
  "description": "Corteza Automation Script Runner",
  "version": "2021.9.8",
  "license": "Apache-2.0",
  "contributors": [
    "Denis Arh <denis.arh@crust.tech>"
  ],
  "private": true,
  "main": "dist/app.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "serve:dev": "ts-node-dev --debounce 200 --transpile-only --respawn --ignore-watch node_modules --ignore-watch usr --ignore-watch dist ./src/server.ts",
    "serve": "ts-node ./src/server.ts",
    "lint": "eslint src/** --ext .js,.ts",
    "test:unit": "mocha",
    "test:unit:cc": "nyc mocha",
    "cdeps": "yarn upgrade @cortezaproject/corteza-js https://github.com/cortezaproject/corteza-protobuf#develop"
  },
  "dependencies": {
    "@cortezaproject/corteza-js": "^2021.9.8",
    "@cortezaproject/corteza-protobuf": "https://github.com/cortezaproject/corteza-protobuf#develop",
    "@grpc/proto-loader": "^0.5.1",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@sentry/node": "^5.14.2",
    "@types/chai": "^4.2.6",
    "@types/chai-as-promised": "^7.1.2",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/pino": "5.15.3",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "axios": "^0.21.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "glob": "^7.1.6",
    "grpc": "^1.24.1",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.21",
    "minimalist": "^1.0.0",
    "mocha": "^6.2.2",
    "moment": "2.25.3",
    "node-watch": "^0.6.3",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "pino": "5.16.0",
    "pino-pretty": "^3.5.0",
    "serialize-javascript": "^3.1.0",
    "sinon": "7.4.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.2"
  },
  "resolutions": {
    "**/**/yargs-parser": "^13.1.2",
    "**/**/moment": "2.25.3"
  },
  "devDependencies": {
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "functions": 90,
    "statements": 90,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src/**/*.ts"
    ]
  },
  "engines": {
    "node": ">=12.14.0"
  }
}
